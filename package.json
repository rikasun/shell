{
  "name": "shell",
  "version": "0.0.0",
  "homepage": "https://cased.com",
  "private": true,
  "scripts": {
    "build:all": "nx build && yarn run build:storybook",
    "build:prod": "nx build --prod",
    "build:storybook": "nx run build-storybook --project=ui",
    "build": "nx build",
    "charts": "yarn charts:build && yarn charts:server",
    "charts:build": "yarn charts:doc && yarn charts:lint && yarn charts:test",
    "charts:doc": "helm-docs --chart-search-root=charts",
    "charts:lint": "ct lint --config ct.yaml",
    "charts:test": "ct install --config ct.yaml",
    "charts:server": "yarn charts:doc && yarn charts:install && yarn charts:port-forward",
    "charts:install": "bash ./.devcontainer/script/charts-install .devcontainer/helm/shell-dev.yaml ./charts/cased-shell/",
    "charts:port-forward": "bash ./.devcontainer/script/charts-port-forward",
    "postcharts:port-forward": "pkill -f port-forward",
    "cli:auth": "bash ./.devcontainer/script/cased-cli-auth",
    "charts:uninstall": "helm uninstall shell-dev",
    "ci:local:commit": "pretty-quick --staged",
    "ci:local:push": "yarn run lint:js && yarn run lint:css",
    "e2e:cloud": "cypress install; nx e2e shell-e2e --browser chrome --base-url http://localhost:4200",
    "e2e": "cypress install; nx e2e shell-e2e --watch --base-url http://localhost:4200",
    "install-git-hooks": "npx husky install",
    "lint:css": "stylelint \"**/*.scss\" --max-warnings=0",
    "lint:js": "nx run-many --all --target=lint --max-warnings=0",
    "lint": "nx lint",
    "migrate": "node_modules/.bin/node-pg-migrate -j js --no-check-order",
    "prettier:check": "prettier --check \"**/*\" --ignore-unknown",
    "prettier": "prettier --write .",
    "seed-db": "python seed.py",
    "start-frontend": "nx serve",
    "start": "./local-run",
    "storybook": "nx run storybook --project=ui",
    "test-migrate": "DATABASE_URL=postgres://cased:shell@127.0.0.1:5536/cased-shell-test node_modules/.bin/node-pg-migrate -j js --no-check-order",
    "test:all": "nx run-many --all --target=test --coverage --parallel",
    "test:remotes": "nx test --project=remotes --watch",
    "test:shell": "nx test --project=shell --watch",
    "test:ui": "nx test --project=ui --watch",
    "test:redux": "nx test --project=redux --watch",
    "test": "nx test --codeCoverage",
    "watch-js": "ESBUILD_WATCH=1",
    "watch-styles": "node_modules/.bin/tailwindcss -c tailwind.config.js -i shell/static/css/tailwind.css -o shell/static/css/style.css --watch",
    "g:component": "nx g @nrwl/react:component $1 --globalCss --export false",
    "g:ui:component": "nx g @nrwl/react:component $1 --globalCss --export --project ui "
  },
